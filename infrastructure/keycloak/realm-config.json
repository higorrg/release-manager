  "clients": [
    {
      "clientId": "release-manager-backend",
      "name": "Release Manager Backend",
      "description": "Backend API for Release Manager",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "bearerOnly": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true
    },
    {
      "clientId": "release-manager-frontend",
      "name": "Release Manager Frontend",
      "description": "Frontend SPA for Release Manager",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "bearerOnly": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "redirectUris": [
        "http://localhost:4200/*",
        "http://frontend/*"
      ],
      "webOrigins": [
        "http://localhost:4200",
        "http://frontend"
      ],
      "defaultRoles": ["viewer"],
      "fullScopeAllowed": true
    }
  ],
  "identityProviders": [
    {
      "alias": "azure-ad",
      "providerId": "oidc",
      "enabled": true,
      "updateProfileFirstLogin": true,
      "trustEmail": true,
      "storeToken": false,
      "addReadTokenRoleOnCreate": false,
      "authenticateByDefault": false,
      "linkOnly": false,
      "firstBrokerLoginFlowAlias": "first broker login",
      "config": {
        "clientId": "${AZURE_AD_CLIENT_ID:your-azure-ad-client-id}",
        "clientSecret": "${AZURE_AD_CLIENT_SECRET:your-azure-ad-client-secret}",
        "authorizationUrl": "https://login.microsoftonline.com/${AZURE_AD_TENANT_ID:your-tenant-id}/oauth2/v2.0/authorize",
        "tokenUrl": "https://login.microsoftonline.com/${AZURE_AD_TENANT_ID:your-tenant-id}/oauth2/v2.0/token",
        "userInfoUrl": "https://graph.microsoft.com/oidc/userinfo",
        "jwksUrl": "https://login.microsoftonline.com/${AZURE_AD_TENANT_ID:your-tenant-id}/discovery/v2.0/keys",
        "issuer": "https://login.microsoftonline.com/${AZURE_AD_TENANT_ID:your-tenant-id}/v2.0",
        "validateSignature": "true",
        "useJwksUrl": "true",
        "guiOrder": "1",
        "syncMode": "IMPORT"
      }
    }
  ]
}